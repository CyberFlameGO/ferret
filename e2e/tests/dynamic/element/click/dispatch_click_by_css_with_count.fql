LET url = @lab.cdn.dynamic + "/#/forms"
LET page = DOCUMENT(url, true)

WAIT_ELEMENT(page, "form")

LET input = ELEMENT(page, "#text_input")

DISPATCH "input" IN input WITH "Foo"
DISPATCH "click" IN page OPTIONS { selector: "#text_input", count: 2 }
DISPATCH "input" IN input WITH "Bar"

WAIT(100)

RETURN T::EQ(input.value, "Bar")